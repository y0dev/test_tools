{
  "metadata": {
    "name": "Arduino Uno",
    "description": "Arduino Uno development board",
    "created": "2025-10-12T01:33:35.268760",
    "version": "1.0"
  },
  "serial": {
    "port": "COM4",
    "baud": 9600,
    "timeout": 1.0,
    "parity": "N",
    "stopbits": 1,
    "bytesize": 8
  },
  "logging": {
    "log_directory": "./output/serial_logs",
    "log_format": "timestamp,data",
    "timestamp_format": "%Y-%m-%d %H:%M:%S.%f",
    "auto_create_dirs": true,
    "use_date_hierarchy": true,
    "date_format": "%Y/%m_%b/%m_%d"
  },
  "data_parsing": {
    "enabled": true,
    "patterns": [
      {
        "name": "sensor_data",
        "description": "Extract sensor readings",
        "regex": "SENSOR:\\s*(\\w+):\\s*(\\d+(?:\\.\\d+)?)",
        "type": "sensor",
        "extract_groups": [
          1,
          2
        ],
        "labels": [
          "sensor_name",
          "value"
        ]
      }
    ],
    "output_formats": [
      "json",
      "csv",
      "txt"
    ],
    "save_raw_data": true
  },
  "filters": {
    "min_data_length": 1,
    "max_data_length": 1000,
    "exclude_patterns": [
      "^\\s*$",
      "^\\r?\\n$"
    ],
    "include_patterns": []
  }
}